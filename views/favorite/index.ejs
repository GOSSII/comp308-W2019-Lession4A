<% include ../partials/header.ejs %>

<!-- Content goes here -->

<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
        <h1><%= title %></h1>
      <br />
      <table class="table table-bordered table-striped table-hover">
        <!-- Header Row-->
        <tr>
          <th class="text-center">Title</th>
          <th class="text-center">favorite</th>
          
        </tr>

        <!-- Template Row-->
        <% for(let count = 0; count < favoriteList.length; count++) { %>
        <tr>
          <td class="text-center"><%= favoriteList[count].Title%></td>
          <td class="text-center"><%= favoriteList[count].Favourite%></td>
        </tr>
        <% } %>
      </table>
    </div>
  </div>
</div>

